# Type: Arguments
alias ls='ls -laGh'
alias tail='tail -F'

# Type: Shorten
alias cdd='cd -'
alias ..='cd ..'
alias f='find . -name'
alias g='git'

alias r='rake'
alias rmig='rake db:migrate'
alias be='bundle exec'
alias bi='bundle install'

# Type: Scripting
function discover {
  history | awk '{a[$2]++}END{for(i in a){print a[i] " " i}}' | sort -rn | head
}
